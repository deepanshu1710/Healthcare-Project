{
  "version": 2,
  "builds": [
    {
      "src": "healthcare/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "requirements.txt",
          "healthcare/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "healthcare/wsgi.py"
    }
  ]
}
